<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>HelloWorld</servlet-name>
    <servlet-class>aiss.HelloWorldServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloWorld</servlet-name>
    <url-pattern>/helloworld</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <error-page>
    <exception-type>java.lang.Throwable</exception-type>
    <location>/error.jsp</location>
  </error-page>
  <servlet>
    <servlet-name>NewSearchController</servlet-name>
    <servlet-class>aiss.controller.ign.NewSearchController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NewSearchController</servlet-name>
    <url-pattern>/newsearch</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>TwitchSearchController</display-name>
    <servlet-name>TwitchSearchController</servlet-name>
    <servlet-class>aiss.controller.twitch.TwitchSearchController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TwitchSearchController</servlet-name>
    <url-pattern>/TwitchSearchController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>TwitchStreamController</display-name>
    <servlet-name>TwitchStreamController</servlet-name>
    <servlet-class>aiss.controller.twitch.TwitchStreamController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TwitchStreamController</servlet-name>
    <url-pattern>/TwitchStreamController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GiantbombController</display-name>
    <servlet-name>GiantbombController</servlet-name>
    <servlet-class>aiss.controller.giantbomb.GiantbombController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GiantbombController</servlet-name>
    <url-pattern>/games</url-pattern>
  </servlet-mapping>
</web-app>
